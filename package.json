{
  "name": "angular-pwa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_prod": "ng serve --prod",
    "start_prod_aot": "ng serve --prod --aot",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --base-href=\"/angular-7-pwa/\" --deploy-url=\"\" && gh-pages -d dist/my-first-app",
    "deploy_prod": "ng build --prod --base-href=\"/angular-7-pwa/\" --deploy-url=\"\" && gh-pages -d dist/angular-pwa",
    "deploy_prod_aot": "ng build --prod --aot --base-href=\"/angular-7-pwa/\" --deploy-url=\"\" && gh-pages -d dist/angular-pwa",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/angular-pwa/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.9",
    "@angular/common": "^7.2.9",
    "@angular/compiler": "^7.2.9",
    "@angular/core": "^7.2.9",
    "@angular/forms": "^7.2.9",
    "@angular/http": "^7.2.9",
    "@angular/platform-browser": "^7.2.9",
    "@angular/platform-browser-dynamic": "^7.2.9",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "^7.2.9",
    "@angular/service-worker": "^7.2.9",
    "core-js": "^2.6.5",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.6",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/language-service": "^7.2.9",
    "@types/jasmine": "^3.3.11",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^11.11.3",
    "codelyzer": "^5.0.0-beta.1",
    "gh-pages": "^2.0.1",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.2.4"
  }
}
